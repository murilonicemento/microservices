        "Period": "1m",
        "HttpStatusCode": 429,
        "EnableRateLimiting": true,
        "PeriodTimespan": 30,
        "ClientWhitelist": [
          "Client1",
          "Client2"
        ],
        "ClientIdHeader": "Client-Id"
      }
    },
